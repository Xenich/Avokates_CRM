@{
    ViewData["Title"] = "Home Page";
}
    <div style="margin:15px">
        <a href="/Home/NewCase">Создать новое дело</a>
    </div>
    <div id="table">
        <label id="errorLabel" style='display:none'>Ошибка</label>
        <h3>Список дел</h3>
        <table class="table table-striped table-bordered table-scrolling table_sort" width="100%" cellspacing="0">
            <thead style="">
                <tr>
                    <th>
                        Дело #
                    </th>
                    <th>
                        Название
                    </th>
                    <th>
                        Создатель
                    </th>
                    <th>
                        Дата создания
                    </th>

                    <th>
                        Дата обновления
                    </th>
                    <th>
                        В архив
                    </th>
                </tr>
            </thead>

            <tbody id="CasesTableBody" v-cloak>
                <tr v-for="_case in casesList">
                    <td>{{_case.idPerCompany}}</td>
                    <td><a target="_blank" v-bind:href="'Case?id='+_case.idPerCompany">{{_case.caseTitle}}</a></td>
                    <td>{{_case.caseOwner}}</td>
                    <td>{{_case.createDate}}</td>
                    <td>{{_case.updateDate}}</td>
                    <td>
                        <a v-bind:href="'CaseToArchive?uid='+_case.uid">Переместить в архив</a>
                    </td>
                </tr>
            </tbody>

        </table>
    </div>

<script src="~/lib/jquery/dist/jquery.js"></script>
<script src="~/js/Index.js"></script>