@{
    ViewData["Title"] = "Home Page";
}

<div id="table">
    <table  class="table table-striped table-bordered table-scrolling table_sort" width="100%" cellspacing="0">
        <thead style="">
            <tr>
                <th>
                    Дело #
                </th>
                <th>
                    Название
                </th>
                <th>
                    Создатель
                </th>
                <th>
                    Дата создания
                </th>

                <th>
                    Дата обновления
                </th>
            </tr>
        </thead>

        <tbody id="CasesTableBody">
            <tr v-for="_case in casesList">
                <td>{{_case.idPerCompany}}</td>
                <td><a v-bind:href="'/Case?id='+_case.uid">{{_case.caseTitle}}</a></td>
                <td>{{_case.caseOwner}}</td>
                <td>{{_case.createDate}}</td>
                <td>{{_case.updateDate}}</td>
            </tr>
        </tbody>

    </table>
</div>

<script src="https://cdn.jsdelivr.net/npm/vue/dist/vue.js"></script>
<script src="~/lib/jquery/dist/jquery.js"></script>
@*<script src="~/js/main.js"></script>*@
<script src="~/js/Index.js"></script>